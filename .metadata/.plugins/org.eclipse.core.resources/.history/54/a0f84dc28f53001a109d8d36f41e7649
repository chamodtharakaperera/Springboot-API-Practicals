package com.nibm.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class MvcController {
	@RequestMapping("/index")
	public String index()
	{
		return "index";
	}
	@RequestMapping("/next")
	public string next()
	{
		
		return "next";
	}
}
